class Solution:
  def romanToInt(self, s: str) -> int:
   dicti = { 'I': 1 , 'V': 5 , 'X': 10 , 'L' : 50 ,'C':100, 'D':500, 'M':1000}
 
   n = len(s)
   sums = 0
   i = 0
   
   while i < n:
    if i < n-1 and dicti[s[i]] < dicti[s[i+1]]:
        sums += dicti[s[i+1]] - dicti[s[i]]
        i += 2
    else:
        sums += dicti[s[i]]
        i += 1

   return  sums
    


     
