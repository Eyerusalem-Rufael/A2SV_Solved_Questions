class Solution:
    def intersection(self, nums1: List[int], nums2: List[int]) -> List[int]:
        # unique = set(nums1)
        # x = set(nums2)
        # result = []
        # for num in x:
        #     if num in unique:
        #         result.append(num)
        # return result

        nums1 = set(nums1)
        nums2 = set(nums2)
        
        return list(nums1 & nums2)


