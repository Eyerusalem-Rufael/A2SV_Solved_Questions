class Solution:
    def topKFrequent(self, nums: List[int], k: int) -> List[int]:
        # def frequency(x):
        #     return x[1]

        freq = Counter(nums)
        sorted_occurance = sorted(freq.items() , key = lambda x : x[1] , reverse = True)
       # print(sorted_occurance)
        return [i for i,_ in sorted_occurance[:k]]

        
