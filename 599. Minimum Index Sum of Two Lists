
class Solution:
    def findRestaurant(self, list1: List[str], list2: List[str]) -> List[str]:
        minLen = float('inf')
        arr = []

        for idx1,elt1 in enumerate(list1):
            for idx2,elt2 in enumerate(list2):
                if elt1 == elt2:
                    indexSum = idx1+idx2
                    if indexSum < minLen:
                        minLen = indexSum
                        arr = [elt1]
                    elif indexSum == minLen:
                        arr.append(elt1)
        return arr


