class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        # S = sorted(s)
        # T = sorted(t)
        # if S == T:
        #     return True
        # else:
        #     return False
        if len(s) != len(t):
            return False

        for i in set(s):
            if s.count(i) != t.count(i):
                return False
        else:
            return True
        
