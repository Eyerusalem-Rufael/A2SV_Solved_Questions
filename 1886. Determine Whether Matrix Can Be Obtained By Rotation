class Solution:
    def findRotation(self, mat: List[List[int]], target: List[List[int]]) -> bool:
        for _ in range(4): # we have to rotate 4X 0 90 180 270
        
            if mat == target:
                return True
            mat = [list(row)[::-1] for row in zip(*mat)]
            
        else:
            return False
    
        
