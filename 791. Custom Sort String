class Solution:
    def customSortString(self, order: str, s: str) -> str:
        s_count = Counter(s)
        s = list(s)
        res = []

        for i in order:
            if i in s:
                target = i * s_count[i]
                res.extend(target)
                del s_count[i]

        for i in s_count:
            if i not in res:
                res.extend(i * s_count[i])
        
        return ''.join(res)
    
