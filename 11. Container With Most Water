class Solution:
    def maxArea(self, height: List[int]) -> int:
        left = 0
        right = len(height) -1
        max_area = float(-inf)

        while left <= right:

            length = min(height[left],height[right])
            width = right - left
            area = length*width
                   
            if height[left] <= height[right]:
                left+=1
            else:
                right-=1
            max_area = max(max_area,area)
            
        return max_area
