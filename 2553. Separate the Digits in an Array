class Solution:
    def separateDigits(self, nums: List[int]) -> List[int]:
        res = []
        for num in nums:
            digits = []

            while num > 0:
                digits.append(num % 10)
                num //= 10
            res.extend(reversed(digits))

        return res

    # res = []
    #     for num in nums:
    #         digit = str(num)
    #         for i in digit:
    #             res.append(int(i))
    #     return res
        
