class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        # for i in range(len(nums)): 
        #     for j in range(len(nums)): 
        #         if i != j and nums[i] + nums[j] == target:
        #             return [i,j]
       
        dic =  {}  
        for i in range(len(nums)):
            compl = target - nums[i] 
            if compl in dic:
                return [dic[compl],i] 
            dic[nums[i]] = i 
