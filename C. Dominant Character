test_case = int(input())
 
for _ in range(test_case):
    length = int(input())
    strs =  input()
 
    if 'aa' in strs:
        print(2)
    elif 'aba' in strs or 'aca' in strs:
        print(3)
    elif 'abca' in strs or 'acba' in strs:
        print(4)
    elif 'abbacca' in strs or 'accabba' in strs:
        print(7)
    else:
        print(-1)
